{
  "name": "website_alejandro-rivera.com",
  "version": "2.0.0",
  "description": "updated version of my website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexuko/Website_alejandro-rivera.com.git"
  },
  "keywords": [
    "alejandro",
    "rivera",
    "web",
    "developer",
    "software",
    "freelance",
    "UX",
    "UI",
    "AWS"    
  ],
  "author": "Alejandro Rivera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexuko/Website_alejandro-rivera.com/issues"
  },
  "homepage": "https://github.com/alexuko/Website_alejandro-rivera.com#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
